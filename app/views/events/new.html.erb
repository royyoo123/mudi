<div class="container container-form">
	<%= simple_form_for @event do |f| %>
	<%= f.input :name %>
	<%= f.input :price %>
	<%= f.input :address %>
	<%= f.input :description %>
	<%= f.input :start_date, as: :datetime, html5: true %>
	<%= f.input :photos, as: :file, input_html: { multiple: true } %>
	<%= f.button :submit %>
<% end %>
</div>